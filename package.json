{
  "name": "streamscribe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:mobile": "next dev --hostname 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "export": "next build && next export",
    "build:analyze": "ANALYZE=true next build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:seed": "node scripts/populate-db.js",
    "db:migrate": "node scripts/migrate-db.js",
    "deploy:check": "npm run lint && npm run type-check && npm run build",
    "deploy:staging": "npm run build && npm run start",
    "deploy:production": "npm run build && npm run start"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^3.10.0",
    "@stripe/stripe-js": "^2.2.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "assemblyai": "^4.15.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.0.0",
    "dotenv": "^17.2.1",
    "fs-extra": "^11.3.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "mongodb": "^6.18.0",
    "mongoose": "^8.18.0",
    "next": "14.0.4",
    "next-auth": "^4.24.11",
    "postcss": "^8.4.32",
    "react": "^18",
    "react-dom": "^18",
    "stripe": "^14.10.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.6",
    "typescript": "^5",
    "yt-dlp-exec": "^1.0.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.10",
    "eslint": "^8",
    "eslint-config-next": "14.0.4"
  }
}
